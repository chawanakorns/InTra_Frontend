{
  "cli": {
    "version": ">= 16.17.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      }
    },
    "secrets": {
      "files": [
        {
          "path": "./google-services.json",
          "secret": "GOOGLE_SERVICES_JSON"
        },
        {
          "path": "./config/firebaseConfig.js",
          "secret": "FIREBASE_CONFIG_JS"
        }
      ]
    }
  },
  "submit": {
    "production": {}
  }
}